struct InnerStructType {
  long im1;
  long im2;
};

struct StructType {
  long m1;
  long m2;
  long m3[3];
  InnerStructType inner;
  sequence<long, 3> m4;
};

struct SimpleStruct {
    long m1;
    InnerStructType inner;
    sequence<InnerStructType, 2> inner_seq;
};

union UnionType switch (long) {
    case 0:
    long case1;

    case 1:
    StructType case2;

    default:
    long case_default;
};

